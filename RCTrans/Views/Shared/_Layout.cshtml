                            <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">

                                <ul class="flex-grow-1">
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Autopark">Автопарк</a>
                                    </li>
                                    <li class="nav-item">
